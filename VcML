import csv
import numpy as np

with open('ml_training_data.csv') as csvfile:
    readCSV = csv.reader(csvfile, delimiter=",")
    #print(readCSV)
    locations = []
    location_IDs = []
    for row in readCSV:
        location = row[0]
        location_ID = row[1]

        locations.append(location)
        location_IDs.append(location_ID)

    dict_data = dict(zip(locations, location_IDs))
    #print (dict_data)


